{
    "name": "Dory Defense", 
    "description": "Dory's defense is increased by 50%.", 
    "version": "1.0", 
    "manifest_version": 3, 
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "js": ["thirdparty/jquery.min.js", "email.js"], 
            "run_at": "document_start"
        }, 
        {
            "matches": ["<all_urls>"], 
            "js": ["thirdparty/md5.min.js", "thirdparty/jquery.min.js", "content.js"], 
            "run_at": "document_start"
        }
    ], 
    "permissions": ["storage"], 
    "action": {
        "default_popup": "popup.html"
    }, 
    "browser_action": {
        "default_popup": "popup.html", 
        "default_title": "Dory"
    }, 
    "icons": {
        "128": "resizedicon128.png"
    }
}