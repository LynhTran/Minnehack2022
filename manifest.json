{
    "name": "Dory Defense",
    "description": " ",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["email.js"],
            "run_at": "document_start"
        },
        {
            "matches": ["<all_urls>"],
            "js": ["md5.min.js", "jquery.min.js","content.js"]
        }
    ],
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html"
    },    
      "options_page": "options.html"
}