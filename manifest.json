{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "malicious_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["malicious.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage"]

}