{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "email": {
      "service_worker": "email.js"
    }, 
    "malicious": {
      "service_worker": "malicious.js"
    }, 
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Dory"
    }    
}